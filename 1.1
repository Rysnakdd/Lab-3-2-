#include <stdio.h>
#include <locale.h>
#include <math.h>

int main()
{
	setlocale(LC_CTYPE, "ukr");
	int b, c, a;
	printf("Введіть два натуральних числа - b та c \n");
	scanf_s("%d%d", &b, &c);
	if (b == 1)
	{
		a = 4 * b - 9 * c;
		printf("a = %d\n", a);
	}
	if (b == 2)
	{
		a = sqrt(2 - b * c);
		printf("a = %d\n", a);
	}
	if (b == 3)
	{
		a = b / (c * c);
		printf("a = %d\n", a);
	}
	if (b!=1 && b!=2 && b!=3)
		printf("Введіть інші цілі числа");
	return 0;
}
