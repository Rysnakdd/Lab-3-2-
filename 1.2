#include <stdio.h>
#include <locale.h>
#include <math.h>

int main()
{
	setlocale(LC_CTYPE, "ukr");
	int b, c, a;
	printf("Введiть два натуральних числа - b та c \n");
	scanf_s("%d%d", &b, &c);
	if (b == 1)
	{
		a = 4 * b - 9 * c;
		printf("a = %d\n", a);
	}
	else
	{

		if (b == 2)
		{
			a = sqrt(2 - b * c);
			printf("a = %d\n", a);
		}
		else
		{
			if (b == 3)
			{
				a = b / (c * c);
				printf("a = %d\n", a);
			}
			else
			{
				printf("Введiть iншi цiлi числа");
			}
		}
	}
	return 0;
}
